        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/picturealbum/listing.jsp</from-view-id>
        <navigation-case>
            <from-outcome>sorted</from-outcome>
            <to-view-id>/picturealbum/listing.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>update</from-outcome>
            <to-view-id>/picturealbum/update.jsp</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/picturealbum/update.jsp</from-view-id>
        <navigation-case>
            <from-outcome>updated</from-outcome>
            <to-view-id>/picturealbum/listing.jsp</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>cancel</from-outcome>
            <to-view-id>/picturealbum/listing.jsp</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>

    <!--PROMOTIONS-->
    <navigation-rule>
        <from-view-id>/promotions/create.jsp</from-view-id>
        <navigation-case>
            <from-outcome>created</from-outcome>
            <to-view-id>/promotions/listing.jsp</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>cancel</from-outcome>
            <to-view-id>/promotions/listing.jsp</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/promotions/listing.jsp</from-view-id>
        <navigation-case>
            <from-outcome>sorted</from-outcome>
            <to-view-id>/promotions/listing.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>update</from-outcome>
            <to-view-id>/promotions/update.jsp</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/promotions/update.jsp</from-view-id>
        <navigation-case>
            <from-outcome>updated</from-outcome>
            <to-view-id>/promotions/listing.jsp</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>cancel</from-outcome>
            <to-view-id>/promotions/listing.jsp</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>

    <!--BREAK TIME-->
    <navigation-rule>
        <from-view-id>/breaktime/create.jsp</from-view-id>
        <navigation-case>
            <from-outcome>created</from-outcome>
            <to-view-id>/breaktime/listing.jsp</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>cancel</from-outcome>
            <to-view-id>/breaktime/listing.jsp</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/breaktime/listing.jsp</from-view-id>
        <navigation-case>
            <from-outcome>sorted</from-outcome>
            <to-view-id>/breaktime/listing.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>update</from-outcome>
            <to-view-id>/breaktime/update.jsp</to-view-id>
