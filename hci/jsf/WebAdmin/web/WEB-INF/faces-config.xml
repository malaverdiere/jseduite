<?xml version='1.0' encoding='UTF-8'?>

<!-- =========== FULL CONFIGURATION FILE ================================== -->

<faces-config version="1.2"
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">

    <!--MANAGED BEANS-->
    <managed-bean>
        <managed-bean-name>BreakingNewsManagedBean</managed-bean-name>
        <managed-bean-class>webadmin.breakingnews.BreakingNewsManagedBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>InternalNewsManagedBean</managed-bean-name>
        <managed-bean-class>webadmin.internalnews.InternalNewsManagedBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>PictureAlbumManagedBean</managed-bean-name>
        <managed-bean-class>webadmin.picturealbum.PictureAlbumManagedBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>PromotionManagedBean</managed-bean-name>
        <managed-bean-class>webadmin.promotions.PromotionManagedBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>BreakTimeManagedBean</managed-bean-name>
        <managed-bean-class>webadmin.breaktime.BreakTimeManagedBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>BreakingNewsSorter</managed-bean-name>
        <managed-bean-class>webadmin.breakingnews.BreakingNewsSorter</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>InternalNewsSorter</managed-bean-name>
        <managed-bean-class>webadmin.internalnews.InternalNewsSorter</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>PictureAlbumSorter</managed-bean-name>
        <managed-bean-class>webadmin.picturealbum.PictureAlbumSorter</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>PromotionSorter</managed-bean-name>
        <managed-bean-class>webadmin.promotions.PromotionSorter</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>DataTablePager</managed-bean-name>
        <managed-bean-class>webadmin.util.DataTablePager</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>

    <!--INTERNATIONALIZATION-->
    <application>
        <locale-config>
            <default-locale>fr_FR</default-locale>
            <supported-locale>en_US</supported-locale>
        </locale-config>
        <resource-bundle>
            <base-name>webadmin.ApplicationMessages</base-name>
            <var>bundle</var>
        </resource-bundle>
        <message-bundle>webadmin.JSFMessages</message-bundle>
    </application>

    <!--CONVERTERS-->
    <converter>
        <converter-id>XMLGregorianCalendarConverter</converter-id>
        <converter-class>webadmin.converters.XMLGregorianCalendarConverter</converter-class>
    </converter>
    <converter>
        <converter-id>XMLGregorianCalendarDateConverter</converter-id>
        <converter-class>webadmin.converters.XMLGregorianCalendarDateConverter</converter-class>
    </converter>
    <converter>
        <converter-id>XMLGregorianCalendarTimeConverter</converter-id>
        <converter-class>webadmin.converters.XMLGregorianCalendarTimeConverter</converter-class>
    </converter>
    <converter>
        <converter-id>TargetConverter</converter-id>
        <converter-class>webadmin.converters.TargetConverter</converter-class>
    </converter>
    <converter>
        <converter-id>DayListConverter</converter-id>
        <converter-class>webadmin.converters.DayListConverter</converter-class>
    </converter>
    <converter>
        <converter-id>PromoListConverter</converter-id>
        <converter-class>webadmin.converters.PromoListConverter</converter-class>
    </converter>

    <!--BREAKING NEWS-->
    <navigation-rule>
        <from-view-id>/breakingnews/create.jsp</from-view-id>
        <navigation-case>
            <from-outcome>created</from-outcome>
            <to-view-id>/breakingnews/listing.jsp</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>cancel</from-outcome>
            <to-view-id>/breakingnews/listing.jsp</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/breakingnews/listing.jsp</from-view-id>
        <navigation-case>
            <from-outcome>update</from-outcome>
            <to-view-id>/breakingnews/update.jsp</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>sorted</from-outcome>
            <to-view-id>/breakingnews/listing.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/breakingnews/update.jsp</from-view-id>
        <navigation-case>
            <from-outcome>updated</from-outcome>
            <to-view-id>/breakingnews/listing.jsp</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>cancel</from-outcome>
            <to-view-id>/breakingnews/listing.jsp</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>

    <!--INTERNAL NEWS-->
    <navigation-rule>
        <from-view-id>/internalnews/create.jsp</from-view-id>
        <navigation-case>
            <from-outcome>created</from-outcome>
            <to-view-id>/internalnews/listing.jsp</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>cancel</from-outcome>
            <to-view-id>/internalnews/listing.jsp</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/internalnews/listing.jsp</from-view-id>
        <navigation-case>
            <from-outcome>sorted</from-outcome>
            <to-view-id>/internalnews/listing.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>update</from-outcome>
            <to-view-id>/internalnews/update.jsp</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/internalnews/update.jsp</from-view-id>
        <navigation-case>
            <from-outcome>updated</from-outcome>
            <to-view-id>/internalnews/listing.jsp</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>cancel</from-outcome>
            <to-view-id>/internalnews/listing.jsp</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>

    <!--PICTURE ALBUM-->
