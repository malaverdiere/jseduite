<faces-config version="1.2"
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">
     <navigation-rule>
        <from-view-id>/Welcome.jsp</from-view-id>
        <navigation-case>
            <from-outcome>breakUse</from-outcome>
            <to-view-id>/showBreak.jsp</to-view-id>
        </navigation-case>
         <navigation-case>
            <from-outcome>breakAdmin</from-outcome>
            <to-view-id>/Admin.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
     <managed-bean>
        <managed-bean-name>BreakUseBean</managed-bean-name>
        <managed-bean-class>Bean.BreakUseBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
     <managed-bean>
        <managed-bean-name>BreakAdminBean</managed-bean-name>
        <managed-bean-class>Bean.BreakAdminBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
     <managed-bean>
        <managed-bean-name>AlarmAdmin</managed-bean-name>
        <managed-bean-class>Bean.AlarmAdminBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
     <navigation-rule>
        <from-view-id>/showBreak.jsp</from-view-id>
        <navigation-case>
            <from-outcome>getTodayBreak</from-outcome>
            <to-view-id>/getTodayBreak.jsp</to-view-id>
        </navigation-case>
         <navigation-case>
            <from-outcome>getBreakByDay</from-outcome>
            <to-view-id>/getBreakByDay.jsp</to-view-id>
        </navigation-case>
         <navigation-case>
            <from-outcome>getBreakByPromo</from-outcome>
            <to-view-id>/getBreakByPromo.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>back</from-outcome>
            <to-view-id>/Welcome.jsp</to-view-id>
        </navigation-case>
           <navigation-case>
            <from-outcome>getAllBreak</from-outcome>
            <to-view-id>/getAllBreak.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
     <navigation-rule>
        <from-view-id>/getTodayBreak.jsp</from-view-id>
        <navigation-case>
            <from-outcome>back</from-outcome>
            <to-view-id>/showBreak.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
       <navigation-rule>
        <from-view-id>/getBreakByPromo.jsp</from-view-id>
        <navigation-case>
            <from-outcome>back</from-outcome>
            <to-view-id>/showBreak.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/getBreakByDay.jsp</from-view-id>
        <navigation-case>
            <from-outcome>back</from-outcome>
            <to-view-id>/showBreak.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
      <navigation-rule>
        <from-view-id>/getAllBreak.jsp</from-view-id>
        <navigation-case>
            <from-outcome>back</from-outcome>
            <to-view-id>/showBreak.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
     <navigation-rule>
        <from-view-id>/Admin.jsp</from-view-id>
        <navigation-case>
            <from-outcome>listAllBreaks</from-outcome>
            <to-view-id>/listAllBreaks.jsp</to-view-id>
        </navigation-case>
         <navigation-case>
            <from-outcome>listAllAlarms</from-outcome>
            <to-view-id>/listAllAlarms.jsp</to-view-id>
        </navigation-case>
          <navigation-case>
            <from-outcome>back</from-outcome>
            <to-view-id>/Welcome.jsp</to-view-id>
        </navigation-case>
          <navigation-case>
            <from-outcome>addBreak</from-outcome>
            <to-view-id>/addBreak.jsp</to-view-id>
        </navigation-case>
       </navigation-rule>
       <navigation-rule>
        <from-view-id>/listAllAlarms.jsp</from-view-id>
        <navigation-case>
            <from-outcome>addNewAlarm</from-outcome>
            <to-view-id>/addAlarm.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>editAlarm</from-outcome>
            <to-view-id>/editAlarm.jsp</to-view-id>
          </navigation-case>
          <navigation-case>
            <from-outcome>back</from-outcome>
            <to-view-id>/Admin.jsp</to-view-id>
          </navigation-case>
    </navigation-rule>
       <navigation-rule>
        <from-view-id>/addBreak.jsp</from-view-id>
        <navigation-case>
            <from-outcome>listAllBreaks</from-outcome>
            <to-view-id>/listAllBreaks.jsp</to-view-id>
        </navigation-case>
          <navigation-case>
            <from-outcome>back</from-outcome>
            <to-view-id>/Admin.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
          <navigation-rule>
        <from-view-id>/listAllBreaks.jsp</from-view-id>
        <navigation-case>
            <from-outcome>back</from-outcome>
            <to-view-id>/Admin.jsp</to-view-id>
        </navigation-case>
          <navigation-case>
            <from-outcome>edit</from-outcome>
            <to-view-id>/editBreak.jsp</to-view-id>
         </navigation-case>
          <navigation-case>
            <from-outcome>addNewBreak</from-outcome>
            <to-view-id>/addBreak.jsp</to-view-id>
           </navigation-case>
            <navigation-case>
            <from-outcome>addNewAlarm</from-outcome>
            <to-view-id>/addAlarm.jsp</to-view-id>
           </navigation-case>
      </navigation-rule>
      <navigation-rule>
        <from-view-id>/editBreak.jsp</from-view-id>
        <navigation-case>
            <from-outcome>listAllBreaks</from-outcome>
            <to-view-id>/listAllBreaks.jsp</to-view-id>
        </navigation-case>
         <navigation-case>
            <from-outcome>back</from-outcome>
            <to-view-id>/listAllBreaks.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
     <navigation-rule>
        <from-view-id>/addAlarm.jsp</from-view-id>
        <navigation-case>
            <from-outcome>listAllAlarms</from-outcome>
            <to-view-id>/listAllAlarms.jsp</to-view-id>
        </navigation-case>
         <navigation-case>
            <from-outcome>back</from-outcome>
            <to-view-id>/listAllAlarms.jsp</to-view-id>
        </navigation-case>
       </navigation-rule>
       <navigation-rule>
            <from-view-id>/listAllBreaks.jsp</from-view-id>
            <navigation-case>
            <from-outcome>editAlarm</from-outcome>
            <to-view-id>/editAlarm.jsp</to-view-id>
             </navigation-case>
       </navigation-rule>
        <navigation-rule>
            <from-view-id>/editAlarm.jsp</from-view-id>
            <navigation-case>
            <from-outcome>listAllAlarms</from-outcome>
            <to-view-id>/listAllAlarms.jsp</to-view-id>
            </navigation-case>
             <navigation-case>
            <from-outcome>back</from-outcome>
            <to-view-id>/listAllAlarms.jsp</to-view-id>
            </navigation-case>
       </navigation-rule>
    </faces-config>
