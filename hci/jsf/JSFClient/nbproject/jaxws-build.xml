<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xalan="http://xml.apache.org/xslt" xmlns:webproject2="http://www.netbeans.org/ns/web-project/2" xmlns:jaxws="http://www.netbeans.org/ns/jax-ws/1">
    <!--
                ===================
                JAX-WS WSIMPORT SECTION
                ===================
            -->
    <target name="wsimport-init" depends="init">
        <mkdir dir="${build.generated.dir}/wsimport/client"/>
        <mkdir dir="${build.generated.dir}/wsimport/binaries"/>
        <taskdef name="wsimport" classname="com.sun.tools.ws.ant.WsImport">
            <classpath path="${java.home}/../lib/tools.jar:${j2ee.platform.wsimport.classpath}:${javac.classpath}"/>
        </taskdef>
        <condition property="conf-dir" value="${conf.dir}/" else="">
            <isset property="conf.dir"/>
        </condition>
    </target>
    <target name="wsimport-client-check-BreakTimeManagerService" depends="wsimport-init">
        <condition property="wsimport-client-BreakTimeManagerService.notRequired">
            <available file="${build.generated.dir}/wsimport/client/cn/m1/hebut/jseduite/technical/breaktime/BreakTimeManagerService.java"/>
        </condition>
    </target>
    <target name="wsimport-client-BreakTimeManagerService" depends="wsimport-init,wsimport-client-check-BreakTimeManagerService" unless="wsimport-client-BreakTimeManagerService.notRequired">
        <wsimport sourcedestdir="${build.generated.dir}/wsimport/client" destdir="${build.generated.dir}/wsimport/binaries" wsdl="${basedir}/${conf-dir}xml-resources/web-service-references/BreakTimeManagerService/wsdl/localhost_8080/BreakManager/BreakTimeManagerService.wsdl" catalog="catalog.xml" extension="true" verbose="true" wsdlLocation="http://localhost:8080/BreakManager/BreakTimeManagerService?wsdl" xnocompile="true"/>
    </target>
    <target name="wsimport-client-clean-BreakTimeManagerService" depends="-init-project">
        <delete dir="${build.generated.dir}/wsimport/client/cn/m1/hebut/jseduite/technical/breaktime"/>
    </target>
    <target name="wsimport-client-check-AlarmManagerService" depends="wsimport-init">
        <condition property="wsimport-client-AlarmManagerService.notRequired">
            <available file="${build.generated.dir}/wsimport/client/cn/m1/hebut/jseduite/technical/alarms/AlarmManagerService.java"/>
        </condition>
    </target>
    <target name="wsimport-client-AlarmManagerService" depends="wsimport-init,wsimport-client-check-AlarmManagerService" unless="wsimport-client-AlarmManagerService.notRequired">
        <wsimport sourcedestdir="${build.generated.dir}/wsimport/client" destdir="${build.generated.dir}/wsimport/binaries" wsdl="${basedir}/${conf-dir}xml-resources/web-service-references/AlarmManagerService/wsdl/localhost_8080/jseduite/AlarmManager/AlarmManagerService.wsdl" catalog="catalog.xml" extension="true" verbose="true" wsdlLocation="http://localhost:8080/jseduite/AlarmManager/AlarmManagerService?wsdl" xnocompile="true"/>
    </target>
    <target name="wsimport-client-clean-AlarmManagerService" depends="-init-project">
        <delete dir="${build.generated.dir}/wsimport/client/cn/m1/hebut/jseduite/technical/alarms"/>
    </target>
    <target name="wsimport-client-check-BreakManagerAdminService" depends="wsimport-init">
        <condition property="wsimport-client-BreakManagerAdminService.notRequired">
            <available file="${build.generated.dir}/wsimport/client/cn/m1/hebut/jseduite/technical/breaktime/admin/BreakManagerAdminService.java"/>
        </condition>
    </target>
    <target name="wsimport-client-BreakManagerAdminService" depends="wsimport-init,wsimport-client-check-BreakManagerAdminService" unless="wsimport-client-BreakManagerAdminService.notRequired">
        <wsimport sourcedestdir="${build.generated.dir}/wsimport/client" destdir="${build.generated.dir}/wsimport/binaries" wsdl="${basedir}/${conf-dir}xml-resources/web-service-references/BreakManagerAdminService/wsdl/localhost_8080/BreakManager/BreakManagerAdminService.wsdl" catalog="catalog.xml" extension="true" verbose="true" wsdlLocation="http://localhost:8080/BreakManager/BreakManagerAdminService?wsdl" xnocompile="true"/>
    </target>
    <target name="wsimport-client-clean-BreakManagerAdminService" depends="-init-project">
        <delete dir="${build.generated.dir}/wsimport/client/cn/m1/hebut/jseduite/technical/breaktime/admin"/>
    </target>
    <target name="wsimport-client-check-AlarmAdminService" depends="wsimport-init">
        <condition property="wsimport-client-AlarmAdminService.notRequired">
            <available file="${build.generated.dir}/wsimport/client/cn/m1/hebut/jseduite/technical/alarms/admin/AlarmAdminService.java"/>
        </condition>
    </target>
    <target name="wsimport-client-AlarmAdminService" depends="wsimport-init,wsimport-client-check-AlarmAdminService" unless="wsimport-client-AlarmAdminService.notRequired">
        <wsimport sourcedestdir="${build.generated.dir}/wsimport/client" destdir="${build.generated.dir}/wsimport/binaries" wsdl="${basedir}/${conf-dir}xml-resources/web-service-references/AlarmAdminService/wsdl/localhost_8080/jseduite/AlarmManager/AlarmAdminService.wsdl" catalog="catalog.xml" extension="true" verbose="true" wsdlLocation="http://localhost:8080/jseduite/AlarmManager/AlarmAdminService?wsdl" xnocompile="true"/>
    </target>
    <target name="wsimport-client-clean-AlarmAdminService" depends="-init-project">
        <delete dir="${build.generated.dir}/wsimport/client/cn/m1/hebut/jseduite/technical/alarms/admin"/>
    </target>
    <target name="wsimport-client-generate" depends="wsimport-client-BreakTimeManagerService, wsimport-client-AlarmManagerService, wsimport-client-BreakManagerAdminService, wsimport-client-AlarmAdminService"/>
    <target name="wsimport-client-compile" depends="-pre-pre-compile">
        <webproject2:javac srcdir="${build.generated.dir}/wsimport/client" classpath="${j2ee.platform.wsimport.classpath}:${javac.classpath}" destdir="${build.classes.dir}"/>
        <copy todir="${build.classes.dir}">
            <fileset dir="${build.generated.dir}/wsimport/binaries" includes="**/*.xml"/>
        </copy>
    </target>
</project>
