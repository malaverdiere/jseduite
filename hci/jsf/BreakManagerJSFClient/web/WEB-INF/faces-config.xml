<?xml version='1.0' encoding='UTF-8'?>

<!-- =========== FULL CONFIGURATION FILE ================================== -->

<faces-config version="1.2" 
    xmlns="http://java.sun.com/xml/ns/javaee" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">

    <managed-bean>
        <managed-bean-name>BreakManagedBean</managed-bean-name>
        <managed-bean-class>ManagedBean.BreakManagedBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/listBreak.jsp</from-view-id>
        <navigation-case>
            <from-outcome>home</from-outcome>
            <to-view-id>/welcomeJSF.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/welcomeJSF.jsp</from-view-id>
        <navigation-case>
            <from-outcome>list</from-outcome>
            <to-view-id>/listBreak.jsp</to-view-id>
        </navigation-case>
         </navigation-rule>
      <navigation-rule>
        <from-view-id>/welcomeJSF.jsp</from-view-id>
         <navigation-case>
            <from-outcome>listAllBreak</from-outcome>
            <to-view-id>/listAllBreak.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>break</managed-bean-name>
        <managed-bean-class>ManagedBean.BreakTimeManagedBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/listAllBreak.jsp</from-view-id>
        <navigation-case>
            <from-outcome>edit</from-outcome>
            <to-view-id>/editBreak.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>add</from-outcome>
            <to-view-id>/breakAdd.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>home</from-outcome>
            <to-view-id>/welcomeJSF.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/editBreak.jsp</from-view-id>
        <navigation-case>
            <from-outcome>listAllBreak</from-outcome>
            <to-view-id>/listAllBreak.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>back</from-outcome>
            <to-view-id>/listAllBreak.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/breakAdd.jsp</from-view-id>
        <navigation-case>
            <from-outcome>listAllBreak</from-outcome>
            <to-view-id>/listAllBreak.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>back</from-outcome>
            <to-view-id>/listAllBreak.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
     <managed-bean>
        <managed-bean-name>alarmManage</managed-bean-name>
        <managed-bean-class>ManagedBean.AlarmManagedBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
      <navigation-rule>
         <from-view-id>/welcomeJSF.jsp</from-view-id>
            <navigation-case>
            <from-outcome>manageAlarm</from-outcome>
            <to-view-id>/alarmManage.jsp</to-view-id>
        </navigation-case>
      </navigation-rule>
       <navigation-rule>
         <from-view-id>/listAllAlarms.jsp</from-view-id>
            <navigation-case>
            <from-outcome>back</from-outcome>
            <to-view-id>/alarmManage.jsp</to-view-id>
        </navigation-case>
      </navigation-rule>
      <navigation-rule>
         <from-view-id>/alarmManage.jsp</from-view-id>
            <navigation-case>
            <from-outcome>listAllAlarm</from-outcome>
            <to-view-id>/listAllAlarms.jsp</to-view-id>
           </navigation-case>
            <navigation-case>
            <from-outcome>back</from-outcome>
            <to-view-id>/welcomeJSF.jsp</to-view-id>
           </navigation-case>
        </navigation-rule>
        <navigation-rule>
         <from-view-id>/alarmManage.jsp</from-view-id>
            <navigation-case>
            <from-outcome>alarmbybinding</from-outcome>
            <to-view-id>/alarmbybinding.jsp</to-view-id>
        </navigation-case>
      </navigation-rule>
       <navigation-rule>
         <from-view-id>/alarmbybinding.jsp</from-view-id>
            <navigation-case>
            <from-outcome>showAlarm</from-outcome>
            <to-view-id>/showAlarm.jsp</to-view-id>
           </navigation-case>
            <navigation-case>
            <from-outcome>back</from-outcome>
            <to-view-id>/alarmManage.jsp</to-view-id>
           </navigation-case>
      </navigation-rule>
        <navigation-rule>
         <from-view-id>/showAlarm.jsp</from-view-id>
            <navigation-case>
            <from-outcome>back</from-outcome>
            <to-view-id>/alarmbybinding.jsp</to-view-id>
           </navigation-case>
          </navigation-rule>
       <managed-bean>
        <managed-bean-name>alarmAdmin</managed-bean-name>
        <managed-bean-class>ManagedBean.AlarmAdminBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
       <navigation-rule>
         <from-view-id>/welcomeJSF.jsp</from-view-id>
            <navigation-case>
            <from-outcome>operateAlarm</from-outcome>
            <to-view-id>/operateAlarm.jsp</to-view-id>
        </navigation-case>
      </navigation-rule>
        <navigation-rule>
         <from-view-id>/operateAlarm.jsp</from-view-id>
            <navigation-case>
            <from-outcome>showAllAlarms</from-outcome>
            <to-view-id>/showAllAlarmsAdmin.jsp</to-view-id>
        </navigation-case>
      </navigation-rule>
      <navigation-rule>
         <from-view-id>/operateAlarm.jsp</from-view-id>
            <navigation-case>
            <from-outcome>addNewAlarm</from-outcome>
            <to-view-id>/addAlarmsAdmin.jsp</to-view-id>
           </navigation-case>
           <navigation-case>
            <from-outcome>back</from-outcome>
            <to-view-id>/welcomeJSF.jsp</to-view-id>
           </navigation-case>
      </navigation-rule>
       <navigation-rule>
         <from-view-id>/addAlarmsAdmin.jsp</from-view-id>
            <navigation-case>
            <from-outcome>showAllAlarms</from-outcome>
            <to-view-id>/showAllAlarmsAdmin.jsp</to-view-id>
            </navigation-case>
              <navigation-case>
            <from-outcome>back</from-outcome>
            <to-view-id>/operateAlarm.jsp</to-view-id>
            </navigation-case>
      </navigation-rule>
        <navigation-rule>
         <from-view-id>/showAllAlarmsAdmin.jsp</from-view-id>
            <navigation-case>
            <from-outcome>editAlarm</from-outcome>
            <to-view-id>/editAlarmadmin.jsp</to-view-id>
            </navigation-case>
            <navigation-case>
            <from-outcome>back</from-outcome>
            <to-view-id>/operateAlarm.jsp</to-view-id>
            </navigation-case>
      </navigation-rule>
        <navigation-rule>
         <from-view-id>/editAlarmadmin.jsp</from-view-id>
            <navigation-case>
            <from-outcome>showAllAlarms</from-outcome>
            <to-view-id>/showAllAlarmsAdmin.jsp</to-view-id>
           </navigation-case>
           <navigation-case>
            <from-outcome>back</from-outcome>
            <to-view-id>/showAllAlarmsAdmin.jsp</to-view-id>
           </navigation-case>
      </navigation-rule>
      
</faces-config>
