    <target name="wsimport-init" depends="init">
        <mkdir dir="${build.generated.dir}/wsimport/client"/>
        <taskdef name="wsimport" classname="com.sun.tools.ws.ant.WsImport">
            <classpath path="${libs.jaxws21.classpath}"/>
        </taskdef>
    </target>
    <target name="wsimport-client-CourseCRUDService" depends="wsimport-init">
        <property name="wsdl-CourseCRUDService" location="xml-resources/web-service-references/CourseCRUDService/wsdl/localhost_8080/jSeduite/sandbox/MenuService/CourseCRUDService.wsdl"/>
        <wsimport sourcedestdir="${build.generated.dir}/wsimport/client" destdir="${build.generated.dir}/wsimport/client" wsdl="${wsdl-CourseCRUDService}" catalog="catalog.xml" extension="true" verbose="true" wsdlLocation="http://localhost:8080/jSeduite/sandbox/MenuService/CourseCRUDService?WSDL" xnocompile="true">
            <produces dir="${build.generated.dir}/wsimport/client/fr/unice/i3s/modalis/jseduite/technical/restaurant" includes="CourseCRUDService.java" casesensitive="no"/>
        </wsimport>
    </target>
    <target name="wsimport-client-clean-CourseCRUDService" depends="-init-project">
        <delete dir="${build.generated.dir}/wsimport/client/fr/unice/i3s/modalis/jseduite/technical/restaurant"/>
    </target>
