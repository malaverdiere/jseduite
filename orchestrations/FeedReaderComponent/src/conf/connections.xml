<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<jbi xmlns="http://java.sun.com/xml/ns/jbi" xmlns:ns1="http://modalis.i3s.unice.fr/jSeduite/orchestrations/wsdl/FeedReader" xmlns:ns2="http://modalis.i3s.unice.fr/jSeduite/orchestrations/bpel/FeedReader" xmlns:ns3="http://registry.technical.jSeduite.modalis.i3s.unice.fr/" xmlns:ns4="http://feeds.technical.jSeduite.modalis.i3s.unice.fr/" version="1.0">
<service-assembly>
<identification>
<name>FeedReaderComponent</name>
<description>Represents the Service Assembly of FeedReaderComponent</description>
</identification>
<connections>
<connection>
<consumer endpoint-name="FeedReaderPort" service-name="ns1:FeedReaderService"/>
<provider endpoint-name="FeedReaderPortTypeRole_myRole" service-name="ns2:external"/>
</connection>
<connection>
<consumer endpoint-name="FeedRegistryRole_partnerRole" service-name="ns2:feedRegistry"/>
<provider endpoint-name="FeedRegistryPort" service-name="ns3:FeedRegistryService"/>
</connection>
<connection>
<consumer endpoint-name="RssReaderRole_partnerRole" service-name="ns2:rssReader"/>
<provider endpoint-name="RssReaderPort" service-name="ns4:RssReaderService"/>
</connection>
</connections>
</service-assembly>
</jbi>
