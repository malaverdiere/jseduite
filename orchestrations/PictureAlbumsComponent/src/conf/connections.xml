</connection>
<connection>
<consumer endpoint-name="PicasaWrapperRole_partnerRole" service-name="ns2:picasa"/>
<provider endpoint-name="PicasaWrapperPort" service-name="ns5:PicasaWrapperService"/>
</connection>
<connection>
<consumer endpoint-name="PartnerKeysRole_partnerRole" service-name="ns2:key_server"/>
<provider endpoint-name="PartnerKeysPort" service-name="ns6:PartnerKeysService"/>
</connection>
