<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<jbi xmlns="http://java.sun.com/xml/ns/jbi" xmlns:ns1="http://modalis.i3s.unice.fr/jSeduite/wsdl/TvShows" xmlns:ns2="http://modalis.i3s.unice.fr/jSeduite/bpel/TvShows" xmlns:ns3="http://modalis.i3s.unice.fr/jSeduite/wsdl/CachedFeedReader" xmlns:ns4="http://tv.technical.jSeduite.modalis.i3s.unice.fr/" version="1.0">
<service-assembly>
<identification>
<name>TvShowsComponent</name>
<description>Represents the Service Assembly of TvShowsComponent</description>
</identification>
<connections>
<connection>
<consumer endpoint-name="TvShowsPort" service-name="ns1:TvShowsService"/>
<provider endpoint-name="TvShowsPortTypeRole_myRole" service-name="ns2:external"/>
</connection>
<connection>
<consumer endpoint-name="CachedFeedReaderPortTypeRole_partnerRole" service-name="ns2:cachedFeedReader"/>
<provider endpoint-name="CachedFeedReaderPort" service-name="ns3:CachedFeedReaderService"/>
</connection>
<connection>
<consumer endpoint-name="helper_partnerRole" service-name="ns2:helper"/>
<provider endpoint-name="TvHelperPort" service-name="ns4:TvHelperService"/>
</connection>
</connections>
</service-assembly>
</jbi>
