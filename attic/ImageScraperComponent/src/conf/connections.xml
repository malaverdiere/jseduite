<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<jbi xmlns="http://java.sun.com/xml/ns/jbi" xmlns:ns1="http://modalis.i3s.unice.fr/jSeduite/wsdl/ImageScrapper" xmlns:ns2="http://modalis.i3s.unice.fr/jSeduite/bpel/ImageScrapper" xmlns:ns3="http://flickr.image.technical.jSeduite.modalis.i3s.unice.fr/" xmlns:ns4="http://picasa.image.technical.jSeduite.modalis.i3s.unice.fr/" xmlns:ns5="http://helper.image.technical.jSeduite.modalis.i3s.unice.fr/" version="1.0">
<service-assembly>
<identification>
<name>ImageScraperComponent</name>
<description>Represents the Service Assembly of ImageScraperComponent</description>
</identification>
<connections>
<connection>
<consumer endpoint-name="ImageScrapperPort" service-name="ns1:ImageScrapperService"/>
<provider endpoint-name="ImageScrapperPortTypeRole_myRole" service-name="ns2:external"/>
</connection>
<connection>
<consumer endpoint-name="flickr_partnerRole" service-name="ns2:flickr"/>
<provider endpoint-name="FlickrWrapperPort" service-name="ns3:FlickrWrapperService"/>
</connection>
<connection>
<consumer endpoint-name="picasa_partnerRole" service-name="ns2:picasa"/>
<provider endpoint-name="PicasaWrapperPort" service-name="ns4:PicasaWrapperService"/>
</connection>
<connection>
<consumer endpoint-name="helper_partnerRole" service-name="ns2:helper"/>
<provider endpoint-name="PictureSetPort" service-name="ns5:PictureSetService"/>
</connection>
</connections>
</service-assembly>
</jbi>
