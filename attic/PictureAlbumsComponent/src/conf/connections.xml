<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<jbi xmlns="http://java.sun.com/xml/ns/jbi" xmlns:ns1="http://modalis.i3s.unice.fr/jSeduite/wsdl/PictureAlbum" xmlns:ns2="http://modalis.i3s.unice.fr/jSeduite/bpel/PictureAlbums" xmlns:ns3="http://flickr.image.technical.jSeduite.modalis.i3s.unice.fr/" xmlns:ns4="http://helper.image.technical.jSeduite.modalis.i3s.unice.fr/" xmlns:ns5="http://picasa.image.technical.jSeduite.modalis.i3s.unice.fr/" xmlns:ns6="http://registry.image.technical.jSeduite.modalis.i3s.unice.fr/" version="1.0">
<service-assembly>
<identification>
<name>PictureAlbumsComponent</name>
<description>Represents the Service Assembly of PictureAlbumsComponent</description>
</identification>
<connections>
<connection>
<consumer endpoint-name="PictureAlbumPort" service-name="ns1:PictureAlbumService"/>
<provider endpoint-name="PictureAlbumPortTypeRole_myRole" service-name="ns2:external"/>
</connection>
<connection>
<consumer endpoint-name="flickr_partnerRole" service-name="ns2:flickr"/>
<provider endpoint-name="FlickrWrapperPort" service-name="ns3:FlickrWrapperService"/>
</connection>
<connection>
<consumer endpoint-name="helper_partnerRole" service-name="ns2:helper"/>
<provider endpoint-name="PictureSetPort" service-name="ns4:PictureSetService"/>
</connection>
<connection>
<consumer endpoint-name="picasa_partnerRole" service-name="ns2:picasa"/>
<provider endpoint-name="PicasaWrapperPort" service-name="ns5:PicasaWrapperService"/>
</connection>
<connection>
<consumer endpoint-name="registry_partnerRole" service-name="ns2:registry"/>
<provider endpoint-name="PictureAlbumRegistryPort" service-name="ns6:PictureAlbumRegistryService"/>
</connection>
</connections>
</service-assembly>
</jbi>
