                    </assign>
                    <while name="a30_concat_loop">
                        <condition>$cpt &lt;= count($breakNews_star/ns0:item)</condition>
                        <sequence name="a30_concat_core">
                            <assign name="a30_tech">
                                <copy>
                                    <from>$breakNews_star/ns0:item[$cpt]</from>
                                    <to>$info_star/ns0:item[$infoCpt]</to>
                                </copy>
                            </assign>
                            <assign name="a30_concat_incr">
                                <copy>
                                    <from>$infoCpt + 1</from>
                                    <to variable="infoCpt"/>
                                </copy>
                                <copy>
                                    <from>$cpt + 1</from>
                                    <to variable="cpt"/>
                                </copy>
                            </assign>
                        </sequence>
                    </while>
